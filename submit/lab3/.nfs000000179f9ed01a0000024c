 *  has enough space for the strlen(in) characters of in.
 */[m
[38;5;121mvoid[m
upcase_words([38;5;121mconst[m [38;5;121mchar[m in[], [38;5;121mchar[m out[])
{
  [38;5;121mint[m inWord = [95m0[m;
  [38;5;121mint[m n = strlen(in)-[95m1[m;
  [93mfor[m ([38;5;121mint[m i = [95m0[m; i < n; i++) {
    [93mif[m (!inWord && isalpha(in[i]) && in[i]) {[16;7Hout[i] = toupper(in[i]);
    }
    [93melse[m [93mif[m(in[i] != [95m'/'[m || in[i] != [95m'0'[m) [46m{[m[19;7Hout[i] = in[i];
    [46m}[m
    inWord = isalpha(in[i]);
  }
